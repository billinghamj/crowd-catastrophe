<div class="row">
	<div class="col-sm-12">
		{{#with issue}}
			<h1>{{{name}}}</h1>
			<p>{{{description}}}</p>

			<ul>
				{{#each media}}
					<li data-id="{{{instagramId}}}" data-url="{{{imageUrl}}}">
						<img src="{{{thumbnailUrl}}}" />
					</li>
				{{/each}}
			</ul>
		{{/with}}
	</div>
</div>
